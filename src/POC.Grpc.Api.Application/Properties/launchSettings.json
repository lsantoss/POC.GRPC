{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "POC.Grpc.Api - Development": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "index.html",
      "environmentVariables": {
        "HTTP1_PORT": "5000",
        "HTTP2_PORT": "6000",
        "ASPNETCORE_ENVIRONMENT": "Development",
        "RUNTIME_ENVIRONMENT_NAME": "development"
      },
      "applicationUrl": "http://localhost:5000/",
      "dotnetRunMessages": "true"
    },
    "POC.Grpc.Api - Staging": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "index.html",
      "environmentVariables": {
        "HTTP1_PORT": "5000",
        "HTTP2_PORT": "6000",
        "ASPNETCORE_ENVIRONMENT": "Staging",
        "RUNTIME_ENVIRONMENT_NAME": "staging"
      },
      "applicationUrl": "http://localhost:5000/",
      "dotnetRunMessages": "true"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/index.html",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}